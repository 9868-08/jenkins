pipeline {
    agent {
        docker { image 'node:14-alpine' }
    }
    stages {
        stage('Test') {
            steps {
                sh 'id'
                sh 'git clone git@github.com:9868-08/jenkins.git'
                sh 'cd jenkins/'
                sh 'docker build . --tag hello-worl'
                sh 'docker run hello-worl'
            }
        }
    }
}
